{
  "configuration": {
    "schema": {
      "_subjects_count": {
        "type": "integer"
      },
      "__manifest": {
        "type": "array",
        "properties": {
          "file_name": {
            "type": "string"
          },
          "file_size": {
            "type": "integer"
          }
        }
      },
      "tags": {
        "type": "array"
      },
      "advSearchFilters": {
        "type": "array",
        "default": []
      },
      "_unique_id": {},
      "study_description": {},
      "study_id": {},
      "study_url": {},
      "project_id": {},
      "short_name": {},
      "full_name": {},
      "commons_url": {},
      "commons": {},
      "disease_type": {},
      "primary_site": {},
      "authz": {},
      "subject_id": {},
      "subject_apollo_id": {},
      "subject_race": {},
      "subject_gender": {},
      "subject_ethnicity": {},
      "subject_metastasis": {},
      "subject_cancer_type": {},
      "subject_cancer_grade": {},
      "subject_cancer_stage": {},
      "subject_year_of_birth": {},
      "subject_primary_disease": {},
      "subject_type_of_exposure": {},
      "subject_chemical_agents_of_exposure": {},
      "prometheus_global_id": {},
      "studies": {
            "type": "array"
      }
    },
    "settings": {
      "cache_drs": true
    }
  },
  "adapter_commons": {
    "TCIA-study-level-metadata": {
      "mds_url": "https://externalgen3.prometheus.data-commons.org/",
      "commons_url": "externalgen3.prometheus.data-commons.org",
      "adapter": "gen3",
      "config": {
        "guid_type": "TCIA_study_metadata",
        "study_field": "gen3_discovery"
      },
      "keep_original_fields": false,
      "field_mappings": {
        "authz": "",
        "tags": "path:tags",
        "_unique_id": "path:study_id",
        "study_id": "path:study_id",
        "study_description": "path:study_description",
        "full_name": "path:full_name",
        "short_name": "path:short_name",
        "commons": "TCIA",
        "study_url": "path:study_url",
        "data_source": "TCIA",
        "_subjects_count": {
          "path": "_subjects_count",
          "default": 0
        },
        "__manifest": "",
        "commons_url": "externalgen3.prometheus.data-commons.org",
        "is_synthetic": "No"
      }
    },
    "TCIA-subject-level-metadata": {
      "mds_url": "https://externalgen3.prometheus.data-commons.org/",
      "commons_url": "externalgen3.prometheus.data-commons.org",
      "adapter": "gen3",
      "config": {
        "guid_type": "TCIA_subject_metadata",
        "study_field": "gen3_discovery"
      },
      "keep_original_fields": false,
      "field_mappings": {
        "authz": "",
        "tags": "path:tags",
        "_unique_id": "path:guid",
        "commons": "path:commons",
        "subject_id": "path:subject_id",
        "subject_apollo_id": "path:apollo_id",
        "subject_race": "path:race",
        "subject_gender": {
          "path": "gender",
          "filters": [
            "normalize_value"
          ],
          "filterParams": {
            "normalize_value": {
              "male": "Male",
              "female": "Female",
              "m": "Male",
              "f": "Female",
              "Male": "Male",
              "Female": "Female",
              "not reported": "Not Reported",
              "Not Reported": "Not Reported"
            }
          },
          "default": ""
        },
        "subject_ethnicity": {
          "path": "ethnicity",
          "filters": [
            "normalize_value"
          ],
          "filterParams": {
            "normalize_value": {
              "NOT HISPANIC OR LATINO": "Not Hispanic or Latino",
              "not reported": "Not Reported",
              "Not Reported": "Not Reported",
              "DECLINED TO ANSWER": "Declined to Answer",
              "HISPANIC OR LATINO": "Hispanic or Latino"
            }
          },
          "default": ""
        },
        "subject_metastasis": "",
        "subject_cancer_type": "",
        "subject_cancer_grade": {
          "path": "diagnoses[*].tumor_grade",
          "filters": [
            "normalize_value"
          ],
          "filterParams": {
            "normalize_value": {
              "Grade 1": "Grade I",
              "G1": "Grade I",
              "Grade 2": "Grade II",
              "G2": "Grade II",
              "Grade 3": "Grade III",
              "G3": "Grade III",
              "Grade 4": "Grade IV",
              "G4": "Grade IV"
            }
          },
          "default": ""
        },
        "subject_cancer_stage": "",
        "subject_year_of_birth": "",
        "subject_primary_disease": "",
        "data_source": "TCIA",
        "commons_url": "externalgen3.prometheus.data-commons.org",
        "prometheus_global_id": "path:prometheus_global_id",
        "is_synthetic": "No",
        "studies": {
          "path": "studies.*"
        }
      }
    }
  }
}
